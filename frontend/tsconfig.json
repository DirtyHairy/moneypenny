{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "importHelpers": true,
    "strict": true,
    "noUnusedParameters": false,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "downlevelIteration": true
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts"
  ]
}
